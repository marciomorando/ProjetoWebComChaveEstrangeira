<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>ServletAlimentaCliente</servlet-name>
        <servlet-class>servlets.ServletAlimentaCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletExcluirCliente</servlet-name>
        <servlet-class>servlets.ServletExcluirCliente</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletAlimentaCliente</servlet-name>
        <url-pattern>/ServletAlimentaCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletExcluirCliente</servlet-name>
        <url-pattern>/ServletExcluirCliente</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <error-page>
        <error-code>404</error-code>
        <location>/erro.jsp</location>
    </error-page>
</web-app>
